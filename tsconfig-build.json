{
	"compilerOptions": {
	    "module": "es2015",
		"moduleResolution": "node",
		"target": "es5",
		"stripInternal": true,
		"sourceMap": true,
		"inlineSources": true,
		"declaration": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"removeComments": false,
		"skipLibCheck": true,
		"lib": [
			"es2015",
			"dom"
		],
		"baseUrl": ".",
		"paths": {
			"@angular/core": [
				"node_modules/@angular/core"
			],
			"rxjs/*": [
				"node_modules/rxjs/*"
			]
		}
	},
	"files": [
		"index.ts"
	],
	"typeRoots": [
		"node_modules/@types"
	],
	"angularCompilerOptions": {
		"strictMetadataEmit": true,
		"skipMetadataEmit": false,
		"skipTemplateCodegen": true
	}
}
